{
  "name": "@herodevs/eol-shared",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/herodevs/eol-shared#readme",
  "bugs": {
    "url": "https://github.com/herodevs/eol-shared/issues"
